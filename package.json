{
  "name": "@tyroneross/interface-built-right",
  "version": "0.1.0",
  "description": "Visual regression testing for Claude Code - capture baselines, compare changes, iterate automatically",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyroneross/interface-built-right.git"
  },
  "homepage": "https://github.com/tyroneross/interface-built-right#readme",
  "bugs": {
    "url": "https://github.com/tyroneross/interface-built-right/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ibr": "./dist/bin/ibr.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "plugin",
    "scripts"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js || true",
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "build:cli": "tsup src/bin/ibr.ts --format cjs --outDir dist/bin",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "ibr": "tsx src/bin/ibr.ts",
    "ui": "cd web-ui && npm run dev",
    "snapshot": "tsx src/bin/ibr.ts start",
    "compare": "tsx src/bin/ibr.ts check",
    "diff": "tsx src/bin/ibr.ts check",
    "sessions": "tsx src/bin/ibr.ts list",
    "login": "tsx src/bin/ibr.ts login",
    "logout": "tsx src/bin/ibr.ts logout"
  },
  "keywords": [
    "visual-regression",
    "screenshot",
    "testing",
    "playwright",
    "claude-code",
    "ui-testing",
    "diff",
    "baseline"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "nanoid": "^5.0.9",
    "pixelmatch": "^6.0.0",
    "playwright": "^1.49.1",
    "pngjs": "^7.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
